#ifndef UTILS_S3D
#define UTILS_S3D

/**
 * 
 */
function min(a, b) {
	if (a<b) {
		return a;
	}
	return b;
}

/**
 * 
 */
function max(a, b) {
	if (a>b) {
		return a;
	}
	return b;
}

/**
 * 
 */
function isEventSet(event, maskEvent) {
	var res = event & maskevent;
	if(res == 0){
		return false;
	}
	return true;
}

#endif
